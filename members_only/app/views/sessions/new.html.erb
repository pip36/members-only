<h1> Login </h1>

<%= form_for(:session, url: login_path) do |f| %>
  <%= f.label "email" %>
  <%= f.email_field :email %>
    <br>
  <%= f.label "password" %>
  <%= f.password_field :password %>

  <%= f.submit "Login" %>
<% end %>

<% if !logged_in? %>
<%= link_to "no account? sign up!", new_user_url %>
<% end %>
